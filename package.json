{
  "name": "reactjs-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite --mode dev",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode dev",
    "build:production": "tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "lint:fix": "eslint --fix src --ext ts,tsx",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "docker-build:dev": "docker-compose -f docker-compose.dev.yml build",
    "docker-tag:dev": "docker tag eztekdockerhub.azurecr.io/reactjsstarter-dev:latest eztekdockerhub.azurecr.io/reactjsstarter-dev",
    "docker-push:dev": "docker push eztekdockerhub.azurecr.io/reactjsstarter-dev",
    "deploy:dev": "npm run docker-build:dev && npm run docker-tag:dev && npm run docker-push:dev"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-basic-styles": "^40.0.0",
    "@ckeditor/ckeditor5-build-classic": "^40.0.0",
    "@ckeditor/ckeditor5-editor-classic": "^40.0.0",
    "@ckeditor/ckeditor5-essentials": "^40.0.0",
    "@ckeditor/ckeditor5-link": "^40.0.0",
    "@ckeditor/ckeditor5-paragraph": "^40.0.0",
    "@ckeditor/ckeditor5-react": "^6.1.0",
    "@ckeditor/ckeditor5-theme-lark": "^40.0.0",
    "@ckeditor/vite-plugin-ckeditor5": "^0.1.3",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.8",
    "@mui/material": "^5.14.9",
    "@mui/styled-engine-sc": "^5.14.9",
    "@mui/x-data-grid": "^6.13.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@stomp/stompjs": "^7.0.0",
    "@types/stompjs": "^2.3.8",
    "@uidotdev/usehooks": "^2.3.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "async-mutex": "^0.4.0",
    "classnames": "^2.3.2",
    "formik": "^2.4.4",
    "formik-mui": "^5.0.0-alpha.0",
    "moment": "^2.29.4",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-toastify": "^9.1.3",
    "sockjs-client": "^1.6.1",
    "styled-components": "^5.3.11",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
